# Development configuration for image upload
# Copy this file to .env.local and fill in your values

DEV_BACKEND_URL=http://localhost:3000
DEV_AUTH_TOKEN=your-dev-token-here

# Web app URL for deep linking after file upload
WEB_APP_URL=http://localhost:5173

# Comma-separated list of allowed domains for network access in dev mode
# Example: DEV_ALLOWED_DOMAINS=http://localhost,http://your-site.com
DEV_ALLOWED_DOMAINS=
